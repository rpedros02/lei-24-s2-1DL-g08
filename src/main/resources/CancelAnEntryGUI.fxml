<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="pt.ipp.isep.dei.esoft.project.ui.gui.CancelAnEntryGUI">
    <VBox alignment="CENTER" fillWidth="false" prefHeight="600.0" prefWidth="600.0" spacing="10"
          style="-fx-background-color: #93DB70;">
        <Label alignment="TOP_LEFT" prefHeight="98.0" prefWidth="300.0"
               style="-fx-font-weight: bold; -fx-font-size: 24; -fx-text-fill: #215E21;" text="    GSM: Cancel an Entry"
               textAlignment="CENTER"/>

        <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="44.0" prefWidth="290.0"
               style="-fx-font-size: 16; -fx-text-fill: #333;" text="Select Entry:" textAlignment="CENTER"/>
        <ComboBox prefWidth="150.0"/>

        <Button onAction="#handleCancelAnEntry"
                style="-fx-background-color: #215E21; -fx-text-fill: white; -fx-font-size: 16;"
                text="Cancel an Entry in the Agenda"/>
        <Button fx:id="btnBack" onAction="#handleGsm"
                style="-fx-background-color: rgb(153,102,51); -fx-font-size: 12;" text="Back" textFill="WHITE"/>
    </VBox>
</AnchorPane>
