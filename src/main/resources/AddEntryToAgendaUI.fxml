    <children>
        <VBox alignment="TOP_CENTER" layoutX="8.0" layoutY="7.0" prefHeight="245.0" prefWidth="469.0" spacing="10.0">
            <children>
                <Label text="Enter the title of the entry to add to the agenda:"/>
                <TextField fx:id="entryTitleTextField"/>
                <Label text="Enter the number of members for the team:"/>
                <TextField fx:id="numMembersTextField"/>
                <Button text="Add Entry" onAction="#handleAddEntryToAgenda"/>
            </children>
        </VBox>
    </children>
</AnchorPane>


